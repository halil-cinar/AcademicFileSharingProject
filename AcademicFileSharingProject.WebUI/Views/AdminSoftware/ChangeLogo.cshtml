
@{
    ViewData["Title"] = "PhotoUpdate";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}
@model SoftwareDto
<form method="post" class="row g-3" enctype="multipart/form-data">
    <div class="col-auto">
        <label for="staticEmail2" class="">Yazılım a ait logo seçiniz</label>
    </div>
    <div class="col-auto">
        <label for="inputPassword2" class="visually-hidden">Döküman</label>
        <input type="file" asp-for="Logo" class="form-control" id="inputPassword2" placeholder="Fotoğraf">
    </div>
    <div class="col-auto">
        <button type="submit" class="btn btn-primary mb-3">Güncelle</button>
    </div>

</form>